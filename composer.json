{
    "name": "4mm/laravel-10-scaffold-commands",
    "description": "create all necessary files for crud with one command.",
    "keywords": ["laravel", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "4mm",
            "email": "davidson@4mmtecnologia.com.br"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "illuminate/support": "^10.0",
        "illuminate/console": "^10.0",
        "illuminate/database": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Scaffolding\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Scaffolding\\ScaffoldingServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
